@model List<ValleyDreamsIndia.AchieverDetail>
    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_OtherLayout.cshtml";
    }

    <!-- Start Feature Section -->
    <section id="portfolio" class="portfolio-section-1">
        <div class="container" style="margin-top:5%;">
            <div class="row">
                <div class="col-md-12">
                    <div class="section-title text-center">
                        <h3>Our Winners</h3>
                        <p>Bethuel Multi Trade Pvt. Ltd.</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">

                    <!-- Start Portfolio items -->
                    <ul id="portfolio-list">

                        @if (Model != null)
                        {
                            var i = 1;
                            foreach (var achiever in Model)
                            {
                            <li>
                                <div class="portfolio-item">
                                    <img src="@achiever.AchieverImage" class="img-responsive" alt="" />
                                    <div class="portfolio-caption">
                                        <h4>Results @System.Globalization.CultureInfo.CurrentUICulture.DateTimeFormat.GetMonthName(Convert.ToInt32(achiever.Month))  @achiever.Year</h4>
                                        <a href="#portfolio-modal_@i" data-toggle="modal" class="link-1">
                                            <i style="margin-left:25px;" class="fa fa-magic"></i>
                                        </a>
                                    </div>
                                </div>
                            </li>
                                i = i + 1;
                            }
                        }
                    </ul>
                    <!-- End Portfolio items -->
                </div>
            </div>


        <div id="sidebar">
            <ul id="mainNav">
                <li id="navDashboard" class="nav  @(ViewContext.RouteData.Values["action"].ToString() == "Dashboard" ? "active" : "") ">
                    <span class="icon-home"></span>
                    <a href="/SuperAdmin/Dashboard">Dashboard</a>
                </li>

                <li id="navPages" class="nav @((ViewContext.RouteData.Values["action"].ToString() == "ViewProfile" || (ViewContext.RouteData.Values["action"].ToString() == "EditProfile") || (ViewContext.RouteData.Values["action"].ToString() == "ChangePassword")) ? "active" : "") ">
                    <span class="icon-document-alt-stroke"></span>
                    <a href="javascript:;">Manage Account</a>

                    <ul class="subNav">

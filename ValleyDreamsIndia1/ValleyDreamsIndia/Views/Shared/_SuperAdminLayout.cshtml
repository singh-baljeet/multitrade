                <li id="navType" class="nav @(ViewContext.RouteData.Values["action"].ToString() == "AssignPin" ? "active" : "") ">
                    <span class="icon-pen-alt2"></span>
                    <a href="/SuperAdmin/AssignPin">Assign Pins</a>

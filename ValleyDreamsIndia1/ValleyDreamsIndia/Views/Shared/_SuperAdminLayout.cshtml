    <script>

        function cancelButton(){
            $("#userId").val("");
            return false;
        }

        function achieverCancelButton()
        {
            $("#myfile").val("")
            $("#datepicker2").val("");
            return false;

        }

       function transactionCancelBtn(){
            $("#OldTransactionPassword").val("");
            $("#NewTransactionPassword").val("");
            $("#ConfirmTransactionNewPassword").val("");
            return false;
       }

       function passwordCancelBtn() {
           $("#OldPassword").val("");
           $("#NewPassword").val("");
           $("#ConfirmNewPassword").val("");
           return false;
       }

        $(function () {


            $("#usersTable").on('click', '.anchSendToken', function () {
                debugger;
                var pid = $(this).attr("userid");
                $.ajax({
                    url: '/SuperAdmin/SendToken?pid=' + pid,
                    type: "GET",
                    success: function (response, textStatus, xhr) {
                        if (response == "success") {
                            alert("Token Number send successfully");
                        }
                        
                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        alert(xhr.responseText);
                    }
                });
            });

            $(".ancPayment").on('click', function () {
                var pairs = $(this).attr("pairs");
                var userid = $(this).attr("userid");
                var paidstatus = $("#paidstatus_" + pairs).val();
                var paiddate = $("#paiddate_" + pairs).val();
                var paidremarks = $("#paidremarks_" + pairs).val();

                var fullurl  = '/SuperAdmin/ClearPayment?userid='+userid+'&pairs=' + pairs + '&paidstatus=' + paidstatus + '&paiddate=' + paiddate + '&paidremarks=' + paidremarks;
                $.ajax({
                    url: fullurl,
                    type: "GET",
                    success: function (data) {
                        window.location = "/SuperAdmin/ViewReward?memberId=" + userid;
                    },
                    error: function (xhr) {
                        alert("Error while updating record");
                    }
                });
            })

            $(".reset").click(function () {
                debugger;
                $(this).closest('form').find("input[type=text], textarea").val("");
                return false;
            });


            $(".explore").click(function (e) {
                var usrId = $(this).attr("userId");
                $('<form action="/SuperAdmin/TreeByUserId" method="POST"/>')
                        .append($('<input type="hidden" name="userId" value="' + usrId + '">'))
                        .appendTo($(document.body))
                .submit();
            })


            $("#memberid").bind('blur', function (e) {
                if (e.type === 'blur') {
                    var memberId = $(this).val();
                    $.ajax({
                        url: '/SuperAdmin/GetMemberDetailsById?memberId=' + memberId,
                        dataType: "json",
                        type: "GET",
                        success: function (data) {
                            if (data == "False") {
                                $("#sOtherContributionNumber").text("");
                                $("#sOtherContributionDate").text("");
                                $("#sOtherSponsoredId").text("");
                                $("#sRenewalPinAvailable").text("");
                                $("#renewalPinStatus").text("No Record Found");
                            }
                            else {
                                $("#renewalPinStatus").text("");
                                $("#sOtherContributionNumber").text(data.ContributionNo);
                                $("#sOtherContributionDate").text(data.ContributionDate);
                                $("#sOtherSponsoredId").text(data.SponsoredId);
                                $("#sRenewalPinAvailable").text(data.AvailableRenewalPins);
                            }
                        },
                        error: function (xhr) {
                            $("#renewalPinStatus").text("Error while getting the searched record");
                        }

                    });
                }
            });


    $("#datepicker").datepicker();
    $("#datepicker1").datepicker();
    $("#datepicker2").datepicker();
    $(".datepickerpaid").datepicker();
	$( "#datepicker_inline" ).datepicker();
	$('#colorpickerHolder').ColorPicker({flat: true});
	$('#timepicker').timepicker ({
		showPeriod: true
		, showNowButton: true
		, showCloseButton: true
	});

	$('#timepicker_inline_div').timepicker({
	   defaultTime: '9:20'
	});

	$('#colorSelector').ColorPicker({
		color: '#FF9900',
		onShow: function (colpkr) {
			$(colpkr).fadeIn(500);
			return false;
		},
		onHide: function (colpkr) {
			$(colpkr).fadeOut(500);
			return false;
		},
		onSubmit: function (hsb, hex, rgb, el) {
			$(el).ColorPickerHide ();
		},
		onChange: function (hsb, hex, rgb) {
			$('#colorSelector div').css({ 'background-color': '#' + hex });
			$('#colorpickerField1').val ('#' + hex);
		}
	});

	$('#colorpickerField1').live ('keyup', function (e) {
		var val = $(this).val ();
		val = val.replace ('#', '');
		$('#colorSelector div').css({ 'background-color': '#' + val });
		$('#colorSelector').ColorPickerSetColor(val);
	});

});

    </script>

    
</body>
</html>
@model ValleyDreamsIndia.Common.TreeStructure.Parent
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div id="contentHeader">
    <h1>Binary Tree</h1>
</div> <!-- #contentHeader -->

<div class="container">
    <div class="grid-12">
        <div class="widget widget-table">

            <div class="widget-header">
                <span class="icon-list"></span>
                <h3 class="icon chart">Search User</h3>
            </div>

            @using (Html.BeginForm("TreeByUserId", "Team"))
            {
                        <div class="widget-content">

                            <table class="table table-bordered table-striped">
                                <thead>
                                    <tr>
                                        <th>User Id</th>
                                        <th>
                                            @Html.TextBox("userId", null, new { placeholder= "Enter User Id" , required="required" })
                                        </th>
                                    </tr>
                                </thead>
                            </table>
                        </div> <!-- .widget-content -->
                        <p align="center" style="margin-top:40px;">
                            <span> 
                                <input type="submit" class="btn btn-large btn-blue" value="Search" />
                             </span>

                            <span style="margin-left:10px;">  
							<button class="btn btn-large btn-gray" onclick="return treeCancelButton()">Cancel</button></span>
                        </p>
            }

        </div>





    </div>



    <div class="grid-12">
        <div class="widget widget-table">

            <div class="widget-header">
                <span class="icon-list"></span>
                <h3 class="icon chart">Parent Member Info</h3>
            </div>

            <div class="widget-content">

                <table class="table table-bordered table-striped">
                    <thead>

                        <tr>
                            <th>Sponser Id</th>
                            <th>@ViewBag.Sponsor</th>
                        </tr>
                        <tr>
                            <th>Sponser Name</th>
                            <th>@ViewBag.SponsorName</th>
                        </tr>
                        <tr>
                            <th>User Id</th>
                            <th>@ViewBag.UserName</th>


                        </tr>
                        <tr>
                            <th>Member Name</th>
                            <th>@ViewBag.FullName</th>


                        </tr>

                        <tr>
                            <th>Left</th>
                            <th>@ViewBag.LeftTeam</th>


                        </tr>
                        <tr>
                            <th>Right</th>
                            <th>@ViewBag.RightTeam</th>


                        </tr>
                        <tr>
                            <th>Directs</th>
                            <th>@ViewBag.DirectTeam</th>


                        </tr>







                    </thead>


                </table>


            </div> <!-- .widget-content -->





        </div>
    </div>

    <div class="grid-24">
        <div class="widget widget-table" style="overflow:auto">

            <div class="widget-header">
                <span class="icon-compass"></span>
                <h3>Team Binary Tree</h3>
            </div> <!-- .widget-header -->



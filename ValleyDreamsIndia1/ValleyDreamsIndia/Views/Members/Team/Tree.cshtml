                                                    @Model.LeftChildren.Detail.Name <br />
                                                    <span>User: </span>@Model.LeftChildren.Detail.UserName <br />
                                                    <span>Sponser:  </span>@Model.LeftChildren.Detail.SponsorName<br />
                                                </span>
                                                <button style="margin-left:5px;" class="btn btn-primary explore " 
                                                        userId="@Model.LeftChildren.Detail.UserName">
                                                    <span class="icon-loop"></span>Explore
                                                </button>
                                            }

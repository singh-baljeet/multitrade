                                                    <br>
                                                    <button style="margin-left:5px;" class="btn btn-primary">
                                                        <span class="icon-loop"></span>Explore
                                                    </button>
                                                }
                                            }
                                            else
                                            {
                                                <img src="~/Content/images/stream/defaultavatar_sm.png" />
                                                <br>
                                                <button style="margin-left:5px;" class="btn btn-primary">
                                                    <span class="icon-loop"></span>Explore
                                                </button>
                                            }

                                        }
                                    </div>
                                </th>
                                <th></th>
                                <th>
                                    <div style="float:left;">
                                        <br>
                                        @if (Model.Detail != null)
                                        {
                                            if (Model.RightChildren != null)

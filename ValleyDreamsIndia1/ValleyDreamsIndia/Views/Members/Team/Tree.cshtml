                                            <img src="~/Content/images/stream/defaultavatar_sm.png" />
                                        </div>
										
                                        <span align="center">
                                                @Model.Detail.Name <br /> 
                                            <span>User: </span>@Model.Detail.UserName<br />
                                            <span>Sponser:</span>@Model.Detail.SponsorName<br/>
                                        </span>
										
                                        <span align="center">
                                            <button class="btn btn-primary explore" userId="@Model.Detail.UserName">
                                            <span class="icon-loop"></span>Explore</button>
                                        </span>
                                    }
                                    else
                                    {
                                        <div align="center" style="margin-right:10px;">
                                            <img src="~/Content/images/stream/defaultavatar_sm.png" />
                                        </div>
                                        <div align="center">
                                            <button class="btn btn-primary">
                                                <span class="icon-loop"></span>Explore
                                            </button>
                                        </div>
